/* Attribute Stickers */

.refurb-sticker {
	position: absolute;
	width: 300px;
	text-align: center;
	background: #0000CC;
	z-index: 6;
	margin-left: 140px;
	margin-top: 475px;
	padding: 8px 0;
	font-weight: bold;
	color: #FFFFFF;
	border-radius: 20px;
	-webkit-border-radius: 20px;
	-moz-border-radius: 20px;
	opacity: 0.8;
	font-size: 18px;
	line-height: 22px;
}

.refurb-sticker .refurb-sticker-sub {
	font-size: 16px;
	line-height: 14px;
}

.cash-sticker {
	background: #0099FF;
	font-size: 35px;
	line-height: 35px;
	padding: 35px 0;
}

.free-sticker {
	background: #00DD00;
	font-size: 32px;
	line-height: 32px;
	padding: 52px 0;
}

.demo-sticker {
	background: #FF0000;
	font-size: 36px;
	line-height: 36px;
	padding: 48px 0;
}
.cash-sticker, .free-sticker, .demo-sticker {
	width: 174px;
	margin-left: 40px;
	margin-top: 392px;	
	position: absolute;
	text-align: center;
	font-weight: bold;
	color: #FFFFFF;
	z-index: 6;
	border-radius: 400px;
	-webkit-border-radius: 400px;
	-moz-border-radius: 400px;
	opacity: 0.8;
}

